


<%unless @delivery_challan.size<1%>
<div class="formMain"> 
 <table cellpadding="0" cellspacing="0" width="100%" border="1">
    <tr>
      <td><p><label>S.No</label></p></td>
      <td><p><label>Challan No</label></p></td>
      <td><p><label>Vehicle no</label></p></td>
      <td><p><label>od</label></p></td>
      <td><p><label>thickness</label></p></td>
      <td><p><label>length</label></p></td>
      <td><p><label>Pcs</label></p></td>
      <td><p><label>Mtrs</label></p></td>
      <td><p><label>Rate Mtrs</label></p></td>
      <td><p><label>total</label></p></td>
    


    </tr>
    <%for taxinvoice in 1..@delivery_challan.size%>
      <tr>
      <td style="font-size:11px;"><%=taxinvoice%></td>
        <td><%=collection_select("#{taxinvoice}[taxinvoice]",:deliverychallan_id, @delivery_challan,:id,:delivery_challan_no,{:include_blank => "---Select---"},{:onchange => "deliverychallan_detail(this.value,#{taxinvoice});"})%></td>
        <td><%= text_field_tag "#{taxinvoice}[taxinvoice][vehicle_no]" %></td>
        <td><%= text_field_tag "#{taxinvoice}[taxinvoice][od]" %></td>
        <td><%= text_field_tag "#{taxinvoice}[taxinvoice][thickness]"%></td>
        <td><%= text_field_tag "#{taxinvoice}[taxinvoice][length]" %></td>
        <td><%= text_field_tag "#{taxinvoice}[taxinvoice][pieces]" %></td>
        <td><%= text_field_tag "#{taxinvoice}[taxinvoice][meters]" %></td>
        <td><%= text_field_tag "#{taxinvoice}[taxinvoice][rs_mtrs]",nil,:onblur => "total_tariff_value(#{taxinvoice});" %></td
        <td><%=  text_field_tag "#{taxinvoice}[taxinvoice][total_tariff_value]",nil,:onblur => "total_amount_tax(#{@delivery_challan.size});" %></td>
      </tr>
    <%end%>
    <tr>  
        <td><p><label>Amount</label></p></td>
        <td><p><label>Packing & Forwarding(Rs)</label></p></td>
        <td><p><label>Total Before Duty</label></p></td>
        <td><p><label>Excise Duty(%)</label></p></td>
        <td><p><label>Ed Cess</label></p></td>
        <td><p><label>(SAH) Ed Cess</label></p></td>
        <td><p><label>All Total</label></p></td>
        <td><p><label>VAT / CST (%)</label></p></td>
        <td><p><label>Grand Total</label></p></td>
            
      </tr>
      <tr>
        <td><%= text_field_tag "#{"taxinvoice"}[amount]" %></td>
       <td><%= text_field_tag "#{"taxinvoice"}[packing_forwarding]",nil,:onblur => "total_before_duty();" %></td>
        <td><%= text_field_tag "#{"taxinvoice"}[total_before_duty]" %></td>
      <td><%= text_field_tag "#{"taxinvoice"}[excise_duty]" %></td>
        <td><%= text_field_tag "#{"taxinvoice"}[ed_cess]" %></td>
        <td><%= text_field_tag "#{"taxinvoice"}[sah_ed_cess]",nil,:onblur => "total_all_duty();" %></td>
        <td><%= text_field_tag "#{"taxinvoice"}[total_all]" %></td>
        <td><%= text_field_tag "#{"taxinvoice"}[vat_cst]",nil,:onblur => "grand_total();" %></td>
            <td><%= text_field_tag "#{"taxinvoice"}[grand_total]" %></td>

      </tr>
  </table>
</div>
<%else%>
  No Serials, Please Create Serial First.
<%end%>






