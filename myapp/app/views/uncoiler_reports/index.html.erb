<div class="gridDiv">
  <div class="titleHead">Listing Uncoiler Reports</div>
  <div class="gryBox">
    <div class="linkOuter w45 fr txtR mt5">
      <%= link_to 'New uncoiler_report', new_uncoiler_report_path %>
    </div><br class="clear" />
    <div id="contacts">
      <div id="contact_list" class="contactsDIV">
        <div class="gridTable">
          <table width="100%">
            <tr>
              <th>Date</th>
              <th>Shift</th>
              <th>Operator name</th>
              <th>Slitting coil no</th>
              <th>Tube od</th>
              <th>Width</th>
              <th>Thickness</th>
              <th>Grade</th>
              <th>Type</th>
              <th>Width actual</th>
              <th>Thickness actual</th>
              <th>Grade actual</th>
              <th>Type actual</th>
              <th>Action</th>
            </tr>

            <% for uncoiler_report in @uncoiler_reports %>
              <tr>
                <td><%=h uncoiler_report.date %></td>
                <td><%=h uncoiler_report.shift %></td>
                <td><%=h uncoiler_report.operator_name %></td>
                <td><%=h uncoiler_report.slittingproduction.slitting_coil_no %></td>
                <td><%=h uncoiler_report.tube_od %></td>
                <td><%=h uncoiler_report.width %></td>
                <td><%=h uncoiler_report.thickness %></td>
                <td><%=h uncoiler_report.grade %></td>
                <td><%=h uncoiler_report.type %></td>
                <td><%=h uncoiler_report.width_actual %></td>
                <td><%=h uncoiler_report.thickness_actual %></td>
                <td><%=h uncoiler_report.grade_actual %></td>
                <td><%=h uncoiler_report.type_actual %></td>
                <td>
                  <div class="fr" style="float:left;">
                    <%=  link_to(image_tag('/images/livia_portal/icon_edit.gif',{:alt =>"Edit", :title=>"Edit", :border => 0, :hspace => "0"}),  edit_uncoiler_report_path(uncoiler_report) )%>
                    <%=  link_to(image_tag('/images/livia_portal/icon_reject_1.gif',{:alt =>"DeActivate", :title=>"DeActivate", :confirm => 'Are you sure?',:border => 0, :hspace => "2"}), uncoiler_report, :confirm => 'Are you sure?', :method => :delete ) %>
                  </div>
                </td>
              </tr>
            <% end %>
          </table>
        </div>
      </div></div></div></div>
<br />