
<%= render :partial=>'layouts/supplier_info' %>
<div id="center-column">
  <div class="gridDiv">
    <div class="titleHead">Suppler Information</div>
    <div class="formMain">
      <% form_for(@address) do |f| %>
        <%if(@address.errors.length >0)%>
          <div class='errorCont'>
            <%= error_messages_for(:address)%>
            <%= flash[:error]%>
            <div class="clear"></div>
          </div>
        <%end%>
        <div>
          <!--BOX 1-->
          <div class="mt5">
            <div class="fl w48" style="width: 48%;" >
              <p>
                <%= f.label :company_name %>
                <%= f.text_field :company_name %>
                <div class="clear" id="cl_5"></div>
              
              </p>
              <p>
                <%= f.label :phone_no %>
                <%= f.text_field :phone_no %>
                <div class="clear" id="cl_5"></div>
              </p>
              <p>
                <%= f.label :fax_no %>
                <%= f.text_field :fax_no %>
                <div class="clear" id="cl_5"></div>
              </p>
              <p>
                <%= f.label :email %>
                <%= f.text_field :email %>
                <div class="clear" id="cl_5"></div>
              </p>
              <p>
                <%= f.label :contact_person %>
                <%= f.text_field :contact_person %>
                <div class="clear" id="cl_5"></div>
              </p>
              <p>
                <%= f.label :excise_reg_no %>
                <%= f.text_field :excise_reg_no %>
                <div class="clear" id="cl_5"></div>
              </p>
              <p>
                <%= f.label :range %>
                <%= f.text_field :range %>
                <div class="clear" id="cl_5"></div>
              </p>
              <p>
                <%= f.label :division %>
                <%= f.text_field :division %>
                <div class="clear" id="cl_5"></div>
              </p>
              <p>
                <%= f.label :collectorate %>
                <%= f.text_field :collectorate %>
                <div class="clear" id="cl_5"></div>
              </p>
              <p>
                <%= f.label :bankers %>
                <%= f.text_field :bankers %>
                <div class="clear" id="cl_5"></div>
              </p>
              <p>
                <%= f.label :street %>
                <%= f.text_field :street %>
                <div class="clear" id="cl_5"></div>
              </p>
              <p>
                <%= f.label :city %>
                <%= f.text_field :city %>
                <div class="clear" id="cl_5"></div>
              </p>
              <p>
                <%= f.label :zipcode %>
                <%= f.text_field :zipcode %>
                <div class="clear" id="cl_5"></div>
              </p>
            
            </div>
            <div class="fl w48" >
              <p>
                <%= f.label :ecc_no %>
                <%= f.text_field :ecc_no %>
                <div class="clear" id="cl_5"></div>
              </p>
              <p>
                <%= f.label :vat_tin_no %>
                <%= f.text_field :vat_tin_no %>
                <div class="clear" id="cl_5"></div>
              </p>
              <p>
                <%= f.label :cst_tin_no %>
                <%= f.text_field :cst_tin_no %>
                <div class="clear" id="cl_5"></div>
              </p>
              <p>
                <%= f.label :ifc_no %>
                <%= f.text_field :ifc_no %>
                <div class="clear" id="cl_5"></div>
              </p>
              <p>
                <%= f.label :dgft_no %>
                <%= f.text_field :dgft_no %>
                <div class="clear" id="cl_5"></div>
              </p>
              <p>
                <%= f.label :pan_no %>
                <%= f.text_field :pan_no %>
