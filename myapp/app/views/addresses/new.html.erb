              </p>
              <p>
                <%= f.label :gta_red_no %>
                <%= f.text_field :gta_red_no %>
                <div class="clear" id="cl_5"></div>
              </p>
            
              <p>
                <%= f.label :bank_a_c_no %>
                <%= f.text_field :bank_a_c_no %>
                <div class="clear" id="cl_5"></div>
              </p>
              <p>
                <%= f.label :address %>
                <%= f.text_area :address ,:rows => 3, :cols => 15%>
                <div class="clear" id="cl_5"></div>
              </p><br/>
              <p>
                <%= f.label :state %>
                <%= f.text_field :state %>
                <div class="clear" id="cl_5"></div>
              </p>
            </div>
          </div>
        </div>
        <br class="clear"/>
        <div class="txtC" id="submit_div">
          <%= f.submit "Create" , :class=>'button submit_buttons'%>
          <%=f.submit 'Cancel',{ :class=>'button submit_buttons cancelbutton' }%>
        </div>
        <br class="clear"/>
