

<%= render :partial=>'layouts/supplier_info' %>

<div id="center-column">
<div class="top-bar">
  <%= link_to 'New', new_address_path,:class=>"button" %>
  <h1>Supplier Info</h1>
</div><br />
<div class="table">
  <%= image_tag '/images/bg-th-left.gif',:height=>"7", :width=>"8",:alt=>"",:title=>"",:class=>"left" %>
  <%= image_tag '/images/bg-th-right.gif',:height=>"7", :width=>"7",:alt=>"",:title=>"",:class=>"right" %>
  <table class="listing" cellpadding="0" cellspacing="0" width="100%">
  <tr>
   <th>Company name</th>
    <th>Address one</th>
    <th>Address two</th>
    <th>Phone no</th>
    <th>Fax no</th>
    <th>Email</th>
    <th>Contact person</th>
    <th>Excise reg no</th>
    <!--<th>Range</th>
    <th>Division</th>
    <th>Collectorate</th>-->
    <th>Ecc no</th>
    <th>Vat tin no</th>
    <th>Cst tin no</th>
    <!--<th>Ifc no</th>
    <th>Dgft no</th>-->
    <th>Pan no</th>
    <th>Service tax reg</th>
    <th>Gta red no</th>
    <th>Bankers</th>
    <th>Bank a c no</th>
      <th>Action</th>
  </tr>

<% for address in @addresses %>
  <tr class="<%=cycle("bg1", "bg2")%>">
     <td><%=h address.company_name %></td>
    <td><%=h address.address_one %></td>
    <td><%=h address.address_two %></td>
    <td><%=h address.phone_no %></td>
    <td><%=h address.fax_no %></td>
    <td><%=h address.email %></td>
    <td><%=h address.contact_person %></td>
    <td><%=h address.excise_reg_no %></td>
    <!--<td><%#=h address.range %></td>
    <td><%#=h address.division %></td>
    <td><%#=h address.collectorate %></td>-->
    <td><%=h address.ecc_no %></td>
    <td><%=h address.vat_tin_no %></td>
    <td><%=h address.cst_tin_no %></td>
    <!--<td><%#=h address.ifc_no %></td>
    <td><%#=h address.dgft_no %></td>-->
    <td><%=h address.pan_no %></td>
    <td><%=h address.service_tax_reg %></td>
    <td><%=h address.gta_red_no %></td>
    <td><%=h address.bankers %></td>
    <td><%=h address.bank_a_c_no %></td>
    <td><%= link_to 'Show', address %></td>
    <td>
          <%=  link_to(image_tag('/images/livia_portal/icon_edit.gif',{:alt =>"Edit", :title=>"Edit", :border => 0, :hspace => "0"}),  edit_address_path(address))%>
          <%=  link_to(image_tag('/images/livia_portal/icon_reject_1.gif',{:alt =>"DeActivate", :title=>"DeActivate", :confirm => 'Are you sure?',:border => 0, :hspace => "2"}), address, :confirm => 'Are you sure?', :method => :delete) %>
      </td>
  </tr>
<% end %>
</table>
</div>
<br />
</div>




