<div class="gridDiv">
  <div class="titleHead">New serialize</div>
  <div class="formMain">
    <% form_for(@serialize) do |f| %>
      <%= f.error_messages %>
      <div style="width: 70%">
        <!--BOX 1-->
        <div class="mt5">
        </div>
        <div class="fl w48" >
          <div>
            <p>
              <%= f.label "Customer name"%>
              <%=f.collection_select(:customer_id, Customer.all,:id,:name,{:include_blank => "----------Select----------"})%>
            </p>
            <p>
              <%= f.label :name %>
              <%= f.text_field :name %>
            </p>
          </div>
        </div>

        <div class="fl w48" >
          <div class="txtC" id="submit_div" >
            <%= f.submit "Create" , :class=>'button submit_buttons'%>
            <%=link_to 'Cancel',rawmaterial_categories_path, :class=>'button submit_buttons cancelbutton' %>
          </div>
        </div>
      </div>

      <br class="clear"/>
    </div>
  <% end %>
</div>
